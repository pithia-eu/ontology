<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owlxml="http://www.w3.org/2006/12/owl2-xml#"
>
    <skos:ConceptScheme rdf:about="https://metadata.pithia.eu/ontology/2.2/verticalCrs">
        <skos:prefLabel xml:lang="en">PITHIA Vertical CRS</skos:prefLabel>
        <dc:description>This vocabulary contains the Vertical Coordinate Reference Systems (CRS) used when describing the vertical extend of an Observation (Observation.xml). It actually describes the height (in different units) of the measurement, whether this height is from the Earth center or from the geoid.</dc:description>
        <dc:date>2013-05-31 10:00:00.0</dc:date>
        <owlxml:versionInfo>0.1</owlxml:versionInfo>
        <dc:publisher>PITHIA</dc:publisher>
        <skos:member>
            <skos:Concept rdf:about="https://metadata.pithia.eu/ontology/2.2/verticalCrs/geoid-km">
                <skos:prefLabel xml:lang="en">Geoid height in km</skos:prefLabel>
                <dc:date>2013-05-31 10:00:00.0</dc:date>
                <owlxml:versionInfo>0.1</owlxml:versionInfo>
                <skos:definition xml:lang="en">Height above geoid in km.</skos:definition>
            </skos:Concept>
        </skos:member>
        <skos:member>
            <skos:Concept rdf:about="https://metadata.pithia.eu/ontology/2.2/verticalCrs/geoid-m">
                <skos:prefLabel xml:lang="en">Geoid height in m</skos:prefLabel>
                <dc:date>2013-05-31 10:00:00.0</dc:date>
                <owlxml:versionInfo>0.1</owlxml:versionInfo>
                <skos:definition xml:lang="en">Height above geoid in m.</skos:definition>
            </skos:Concept>
        </skos:member>
        <skos:member>
            <skos:Concept rdf:about="https://metadata.pithia.eu/ontology/2.2/verticalCrs/earthCenter-km">
                <skos:prefLabel xml:lang="en">Earth center in km</skos:prefLabel>
                <dc:date>2013-05-31 10:00:00.0</dc:date>
                <owlxml:versionInfo>0.1</owlxml:versionInfo>
                <skos:definition xml:lang="en">The distance from the Earth center in km.</skos:definition>
            </skos:Concept>
        </skos:member>
        <skos:member>
            <skos:Concept rdf:about="https://metadata.pithia.eu/ontology/2.2/verticalCrs/earthCenter-m">
                <skos:prefLabel xml:lang="en">Earth center in m</skos:prefLabel>
                <dc:date>2013-05-31 10:00:00.0</dc:date>
                <owlxml:versionInfo>0.1</owlxml:versionInfo>
                <skos:definition xml:lang="en">The distance from the Earth center in m.</skos:definition>
            </skos:Concept>
        </skos:member>
        <skos:member>
            <skos:Concept rdf:about="https://metadata.pithia.eu/ontology/2.2/verticalCrs/earthCenter-Re">
                <skos:prefLabel xml:lang="en">Earth center in Re</skos:prefLabel>
                <dc:date>2013-05-31 10:00:00.0</dc:date>
                <owlxml:versionInfo>0.1</owlxml:versionInfo>
                <skos:definition xml:lang="en">The distance from the Earth center in mean Earth Radius (which is 6371 km)..</skos:definition>
            </skos:Concept>
        </skos:member>
        <skos:member>
            <skos:Concept rdf:about="https://metadata.pithia.eu/ontology/2.2/verticalCrs/earthCenter-meanRe-km">
                <skos:prefLabel xml:lang="en">Earth center minus mean Re in Km</skos:prefLabel>
                <dc:date>2013-05-31 10:00:00.0</dc:date>
                <owlxml:versionInfo>0.1</owlxml:versionInfo>
                <skos:definition xml:lang="en">The distance from the Earth center minus the mean Earth Radius (which is 6371 km) in km.</skos:definition>
            </skos:Concept>
        </skos:member>
        <skos:member>
            <skos:Concept rdf:about="https://metadata.pithia.eu/ontology/2.2/verticalCrs/earthCenter-meanRe-m">
                <skos:prefLabel xml:lang="en">Earth center minus mean Re in m</skos:prefLabel>
                <dc:date>2013-05-31 10:00:00.0</dc:date>
                <owlxml:versionInfo>0.1</owlxml:versionInfo>
                <skos:definition xml:lang="en">The distance from the Earth center minus the mean Earth Radius (which is 6371 km) in m.</skos:definition>
            </skos:Concept>
        </skos:member>
    </skos:ConceptScheme>
</rdf:RDF>
